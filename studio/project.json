{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/studio/src",
    "projectType": "application",
    "targets": {
      "build": {
        "executor": "@nrwl/react:build",
        "options": {
          "outputPath": "apps/studio/dist",
          "uglify": true,
          "color": true,
          "profile": false
        },
        "configurations": {
          "production": {}
        }
      },
      "serve": {
        "executor": "@nrwl/react:server",
        "options": {
          "buildTarget": "sanity:build"
        },
        "configurations": {
          "production": {
            "buildTarget": "sanity:build:production"
          }
        }
      },
      "lint": {
        "executor": "@nrwl/linter:eslint",
        "outputs": ["{options.outputFile}"],
        "options": {
          "lintFilePatterns": ["apps/sanity/**/*.{ts,tsx,js,jsx}"]
        }
      },
      "test": {
        "executor": "@nrwl/jest:jest",
        "outputs": ["coverage/apps/sanity"],
        "options": {
          "jestConfig": "apps/sanity/jest.config.ts",
          "passWithNoTests": true
        }
      }
    },
    "tags": []
  }
  